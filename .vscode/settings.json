{
    "autoDocstring.docstringFormat": "google",
    "python.analysis.typeCheckingMode": "basic",
    "cSpell.enableFiletypes": [
        "dockerfile",
        "xml"
    ],
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#ddfdc1",
        "activityBar.background": "#ddfdc1",
        "activityBar.foreground": "#15202b",
        "activityBar.inactiveForeground": "#15202b99",
        "activityBarBadge.background": "#65aafa",
        "activityBarBadge.foreground": "#15202b",
        "commandCenter.border": "#15202b99",
        "sash.hoverBorder": "#ddfdc1",
        "statusBar.background": "#c2fb90",
        "statusBar.foreground": "#15202b",
        "statusBarItem.hoverBackground": "#a7f95f",
        "statusBarItem.remoteBackground": "#c2fb90",
        "statusBarItem.remoteForeground": "#15202b",
        "titleBar.activeBackground": "#c2fb90",
        "titleBar.activeForeground": "#15202b",
        "titleBar.inactiveBackground": "#c2fb9099",
        "titleBar.inactiveForeground": "#15202b99"
    },
    "peacock.remoteColor": "#c2fb90",
    "cSpell.words": [
        "babystepping",
        "bltouch",
        "decel",
        "DPMS",
        "ENDSTOP",
        "Frameless",
        "gcode",
        "klippy",
        "metascan",
        "Momcake",
        "mooncake",
        "printcore",
        "qtobject",
        "Reimplementation",
        "sdbus",
        "setdefaulttimeout",
        "TESTZ",
        "Tickmarks",
        "topbar",
        "unextrude"
